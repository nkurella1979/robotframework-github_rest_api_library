pipeline {
    agent any
    stages {
            stage("Clone") {
                steps {
                    dir('C:/abc') {
                        git 'https://github.com/nkurella1979/robotframework-github_rest_api_library.git'
                    }
                }
            }
            stage("Build") {
                steps {
                    dir('C:/abc/') {
                        bat 'python setup.py install'
                    }
                }
            }
            stage("LAUNCH RIDE"){
                steps {
                   dir('C:/Python27/Scripts') {
                       bat 'python ride.py'
                   }
                }
            }
    }
}
